{
  "name": "nativescript-epub",
  "version": "0.1.0",
  "description": "NativeScript plugin for ePub reader.",
  "main": "epub.js",
  "typings": "index.d.ts",
  "nativescript": {
    "platforms": {
      "android": "2.2.0",
      "ios": "2.2.1"
    }
  },
  "scripts": {
    "build": "tsc",
    "demo.ios": "npm run preparedemo && cd demo && tns emulate ios",
    "demo.android": "npm run preparedemo && cd demo && tns run android",
    "preparedemo": "npm run build && cd demo && tns plugin remove nativescript-epub && tns plugin add .. && tns install",
    "setup": "npm i && cd demo && npm i && cd .. && npm run build && cd demo && tns plugin add .. && cd .."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sean-perkins/nativescript-epub.git"
  },
  "keywords": [
    "NativeScript",
    "JavaScript",
    "ePub Reader",
    "iOS"
  ],
  "author": {
    "name": "Sean Perkins",
    "email": "sean@meetmaestro.com"
  },
  "bugs": {
    "url": "https://github.com/sean-perkins/nativescript-epub/issues"
  },
  "license": "MIT",
  "homepage": "https://github.com/sean-perkins/nativescript-epub",
  "readmeFilename": "README.md",
  "devDependencies": {
    "tns-core-modules": "^2.2.0",
    "tns-platform-declarations": "^2.2.0",
    "typescript": "^1.8.10"
  }
}
